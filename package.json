		"next": "12.0.8",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-redux": "^8.0.1",
		"redux": "^4.2.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.22.2",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^12.1.2",
		"@types/node": "17.0.10",
		"@types/react": "17.0.38",
		"@types/react-dom": "^17.0.16",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"autoprefixer": "^10.4.5",
